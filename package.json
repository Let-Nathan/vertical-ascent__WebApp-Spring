  {
    "name": "vertical-ascent",
    "version": "1.0.0",
    "eslintConfig": {
      "extends": "eslint:recommended",
      "rules": {
        "semi": [
          "error",
          "always"
        ],
        "linebreak-style": [
          "error",
          "windows"
        ],
        "no-tabs": [
          "error",
          {
            "allowIndentationTabs": true
          }
        ],
        "quotes": [
          "error",
          "single"
        ]
      }
    },
    "devDependencies": {
      "eslint": "^8.50.0"
    }
  }
